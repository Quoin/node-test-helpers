{
  "name": "@quoin/node-test-helpers",
  "version": "1.0.0",
  "description": "Test helpers library for nodeJS",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "prepublish": "npm run -s lint && npm test",
    "lint:node": "eslint --fix --ignore-pattern=\"*.test.js\" lib",
    "lint:test": "eslint --fix --config=.eslintrc.test.json \"lib/**/*.test.js\"",
    "lint": "npm run -s lint:node && npm run -s lint:test",
    "test:unit": "mocha \"lib/**/*.unit.test.js\"",
    "test": "npm run test:unit",
    "coverage:unit": "nyc --report-dir=reports/coverage/unit npm run test:unit",
    "coverage": "npm run coverage:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/node-test-helpers.git"
  },
  "keywords": [
    "nodejs",
    "testing",
    "helpers"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/node-test-helpers/issues"
  },
  "homepage": "https://github.com/Quoin/node-test-helpers#readme",
  "dependencies": {
    "chai-http": "4.0.x",
    "dirty-chai": "2.0.x",
    "sinon-chai": "3.1.x"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "latest",
    "@quoin/node-test-helpers-core": "1.0.x",
    "chai": "4.1.x",
    "eslint": "4.19.x",
    "eslint-config-standard": "11.0.x",
    "eslint-plugin-html": "4.0.x",
    "eslint-plugin-import": "2.12.x",
    "eslint-plugin-json": "1.2.x",
    "eslint-plugin-node": "6.0.x",
    "eslint-plugin-promise": "3.7.x",
    "eslint-plugin-standard": "3.1.x",
    "mocha": "5.2.x",
    "nyc": "11.8.x",
    "sinon": "4.5.x"
  }
}
